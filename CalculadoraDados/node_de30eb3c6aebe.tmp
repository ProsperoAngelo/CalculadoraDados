var vetor = []

vetor.push(6)
vetor.push(4)
vetor.push(12)
vetor.push(3)
vetor.push(11)
vetor.push(1)

console.log(`Vetor base: ${vetor}`)

vetor.sort(function(x,y){return x-y})

console.log(`Vetor organizado: ${vetor}`)

var soma = 0



for(let c = 0; c = vetor.length; c++){

soma += vetor[c]

}




console.log(`Soma dos valores do vetor: ${soma}`)